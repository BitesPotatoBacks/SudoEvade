<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.bitespotatobacks.SudoEvade</string>
    <key>ServiceDescription</key>
    <string>Run commands as root without a password</string>
    <key>Program</key>
    <string>/Library/PrivilegedHelperTools/com.bitespotatobacks.SudoEvade.sh</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Library/PrivilegedHelperTools/com.bitespotatobacks.SudoEvade.sh</string>
    </array>
    <key>RunAtLoad</key>
     <true/>
    <key>KeepAlive</key>
    <true/>
    <key>LaunchOnlyOnce</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/com.bitespotatobacks.SudoEvade.stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/com.bitespotatobacks.SudoEvade.stderr.log</string>
</dict>
</plist>
